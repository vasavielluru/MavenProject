pipeline {
    agent any
    tools {
        maven 'Maven'
        
    }
    stages {
       
        

        stage ('Build') {
            steps {
                echo "Building" 
            }
        }
            stage ('Test') {
                steps {
                    echo "Testing"
                }
            }
        stage('build&SonarQube analysis') {
            agent any
            steps{
       
    withSonarQubeEnv('sonar-server') { // If you have configured more than one global server connection, you can specify its name
      sh 'java -version'
      sh 'mvn clean package sonar:sonar'
        }
            }
        }
    }
    
}
